{
  "version": 2,
  "builds": [
    {
      "src": "ggnoteook/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "ggnoteook/build"
      }
    },
    {
      "src": "backend/api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://ggnotebook.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "/backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/ggnotebook/build/$1"
    }
  ]
}
