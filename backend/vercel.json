{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://ggnotebook.vercel.app"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "ggnoteook/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "ggnoteook/build"
      }
    },
    {
      "src": "backend/api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/backend/routes/auth.js"
    },
    {
      "src": "/api/notes/(.*)",
      "dest": "/backend/routes/notes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/ggnotebook/build/$1"
    }
  ]
}
